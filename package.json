{
  "name": "pdf-quiz-generator",
  "version": "1.0.0",
  "description": "PDF를 업로드하여 퀴즈를 생성하는 웹앱",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "start": "node server/server.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "pdf",
    "quiz",
    "education",
    "ai"
  ],
  "author": "",
  "license": "MIT"
}
